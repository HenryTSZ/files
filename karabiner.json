          {
            "description": "Change left_shift to cmd+k if pressed alone in VSCode or NewNewsWire",
            "manipulators": [
              {
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "com\\.microsoft\\.VSCode",
                      "com\\.microsoft\\.VSCodeInsiders",
                      "NetNewsWire"
                    ],
                    "type": "frontmost_application_if"
                  }
                ],
                "from": {
                  "key_code": "left_shift",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_shift"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "k",
                    "modifiers": ["left_command"]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "SpaceFN: Space enables SpaceFN mode (see: https://geekhack.org/index.php?topic=51069.0)",
            "manipulators": [
              {
                "from": {
                  "key_code": "spacebar",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "set_variable": {
                      "name": "spacefn_mode",
                      "value": 1
                    }
                  }
                ],
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "spacefn_mode",
                      "value": 0
                    }
                  }
